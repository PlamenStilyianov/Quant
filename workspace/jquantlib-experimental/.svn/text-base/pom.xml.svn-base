<project
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    
    <modelVersion>4.0.0</modelVersion>
    

    <parent>
        <groupId>org.jquantlib</groupId>
        <artifactId>jquantlib-parent</artifactId>
        <version>0.2.5-SNAPSHOT</version>
        <!-- relativePath>../jquantlib-parent/pom.xml</relativePath -->
    </parent>

    <groupId>org.jquantlib</groupId>
    <artifactId>jquantlib-experimental</artifactId>
    <version>0.2.5-SNAPSHOT</version>
    <packaging>jar</packaging>

    <name>${project.artifactId}</name>
    <url>http://www.jquantlib.org/sites/${project.artifactId}/</url>

    <distributionManagement>
        <!-- NOTE: <site> entry cannot be inherited.
          **       It means to say that you need to copy/paste this text as it is to all projects.
          **       See comments in the parent POM for more information.
          -->
        <site>
            <id>site</id>
            <name>${project.project.artifactId} site generated by Maven</name>
            <url>${props.site.deploy.url}/${project.artifactId}</url>
        </site>
    </distributionManagement>

    <repositories>
        <repository>
            <id>joda-primitives</id>
            <name>joda-primitives</name>
            <url>http://joda-primitives.sourceforge.net/repo/</url>
            <layout>default</layout>
        </repository>
    </repositories>

    <properties>
        <joda-primitives.version>0.6</joda-primitives.version>
    </properties>

    <dependencies>
        <!-- 'compile' scope dependencies -->
        <dependency>
            <groupId>net.jcip</groupId>
            <artifactId>jcip-annotations</artifactId>
            <version>${jcip-annotations.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <version>${slf4j.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.jquantlib</groupId>
            <artifactId>jquantlib</artifactId>
            <version>${project.parent.version}</version>
        </dependency>

        <dependency>
            <groupId>org.joda</groupId>
            <artifactId>joda-primitives</artifactId>
            <version>0.6</version>
            <scope>compile</scope>
        </dependency>

        <!-- 'test' scope dependencies -->
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-simple</artifactId>
            <version>${slf4j.version}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>${junit.version}</version>
            <scope>test</scope>
        </dependency>
    </dependencies>

</project>
